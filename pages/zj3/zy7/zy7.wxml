<text>pages/zj3/zy7/zy7.wxml</text>
<view class='title'>九九乘法表</view>
<view class='con'>
  <view wx:for="{{[1,2,3,4,5,6,7,8,9]}}"  wx:key='i' wx:for-item="i">
    <view class='inline' wx:for="{{[1,2,3,4,5,6,7,8,9]}}" wx:key='i' wx:for-item="j">
      <view wx:if="j<=i">{{i}}*{{j}}={{i*j}}</view>
    </view>
  </view>
</view>
